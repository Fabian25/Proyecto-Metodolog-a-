<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="336.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ModificarEmpleadoController">
   <children>
      <ImageView fitHeight="705.0" fitWidth="1264.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Imagen/fondoPro.jpg" />
         </image>
      </ImageView>
      <Label layoutX="596.0" layoutY="44.0" text="Edit" textFill="WHITE">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="btnADD" layoutX="889.0" layoutY="451.0" mnemonicParsing="false" onAction="#btnActualizar" prefHeight="45.0" prefWidth="184.0" text="SAVE">
         <effect>
            <Lighting bumpInput="$null" diffuseConstant="1.42" specularConstant="1.07" specularExponent="14.88" surfaceScale="0.0">
               <light>
                  <Light.Distant color="#18d718" />
               </light>
            </Lighting>
         </effect>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="txt_Lastname" layoutX="970.0" layoutY="302.0" prefHeight="35.0" prefWidth="194.0" />
      <TextField fx:id="txt_Name" layoutX="970.0" layoutY="241.0" prefHeight="35.0" prefWidth="194.0" />
      <Button layoutX="807.0" layoutY="297.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="125.0" text="Last Name" />
      <Button layoutX="807.0" layoutY="236.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="125.0" text="Name" />
      <TableView fx:id="tblEmpleado" layoutX="30.0" layoutY="194.0" onMouseClicked="#CargarEmpleados" prefHeight="313.0" prefWidth="669.0">
         <columns>
            <TableColumn fx:id="colunmCode" prefWidth="66.0" text="Code" />
            <TableColumn fx:id="ColunmName" prefWidth="73.0" text="Name" />
            <TableColumn fx:id="ColunmLastName" prefWidth="86.0" text="Last Name" />
            <TableColumn fx:id="ColunmIDNumber" prefWidth="87.0" text="ID Number" />
            <TableColumn fx:id="ColunmPhoneNumber" prefWidth="94.0" text="Phone Number" />
            <TableColumn fx:id="ColunmEmail" prefWidth="262.0" text="Email" />
         </columns>
      </TableView>
      <Button layoutX="807.0" layoutY="359.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="125.0" text="Phone Number" />
      <TextField fx:id="txt_Phone" layoutX="970.0" layoutY="364.0" prefHeight="35.0" prefWidth="194.0" />
      <MenuBar layoutY="-2.0" prefHeight="33.0" prefWidth="1253.0">
         <menus>
            <Menu mnemonicParsing="false">
               <graphic>
                  <Button fx:id="BarRegisEmp" mnemonicParsing="false" onAction="#E_BarRegist" text="Register Employee" />
               </graphic>
            </Menu>
            <Menu mnemonicParsing="false">
               <graphic>
                  <Button fx:id="BarEditEmp" cacheShape="false" centerShape="false" focusTraversable="false" mnemonicParsing="false" onAction="#E_BarEdit" scaleShape="false" text="Edit Employee">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </graphic>
            </Menu>
            <Menu mnemonicParsing="false">
               <graphic>
                  <Button fx:id="BarRemoveEmp" mnemonicParsing="false" onAction="#E_BarRemove" text="Remove Employee" />
               </graphic>
            </Menu>
            <Menu mnemonicParsing="false">
               <graphic>
                  <Button fx:id="BarViewEmp" mnemonicParsing="false" onAction="#E_BarView" text="View Employee" />
               </graphic>
            </Menu>
            <Menu mnemonicParsing="false">
               <graphic>
                  <Button fx:id="BarHomeE" mnemonicParsing="false" onAction="#E_Home" text="Home" />
               </graphic>
            </Menu>
         </menus>
      </MenuBar>
      <TextField fx:id="txtBusqueda" layoutX="30.0" layoutY="151.0" onKeyReleased="#Busqueda" />
   </children>
</AnchorPane>
