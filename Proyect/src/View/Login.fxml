<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <ImageView fitHeight="711.0" fitWidth="1512.0" layoutY="-2.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Imagen/fondoPro.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="txt_Usuario" layoutX="154.0" layoutY="311.0" prefHeight="44.0" prefWidth="312.0" promptText="Username">
         <font>
            <Font size="20.0" />
         </font></TextField>
      <PasswordField fx:id="txt_Contra" layoutX="154.0" layoutY="395.0" prefHeight="44.0" prefWidth="312.0" promptText="Password">
         <font>
            <Font size="20.0" />
         </font></PasswordField>
      <Label fx:id="lbl_Recuperar" layoutX="154.0" layoutY="452.0" onMouseClicked="#RecuperarContra" prefHeight="26.0" prefWidth="166.0" text="Forgot password?" textFill="#6268e8">
         <font>
            <Font size="17.0" />
         </font>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Label>
      <Button fx:id="btn_Ingresar" layoutX="216.0" layoutY="510.0" mnemonicParsing="false" onAction="#Ingresar" prefHeight="48.0" prefWidth="144.0" text="Sign In">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Label layoutX="154.0" layoutY="121.0" text="Welcome">
         <font>
            <Font size="70.0" />
         </font>
      </Label>
      <Button fx:id="btnExit" layoutX="1106.0" layoutY="41.0" mnemonicParsing="false" onAction="#Exit" prefHeight="45.0" prefWidth="99.0" text="Exit">
         <effect>
            <Lighting bumpInput="$null" diffuseConstant="1.42" specularConstant="1.07" specularExponent="14.88" surfaceScale="0.0">
               <light>
                  <Light.Distant color="#d72929" />
               </light>
            </Lighting>
         </effect>
      </Button>
   </children>
</AnchorPane>
